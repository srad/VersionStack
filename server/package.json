{
  "name": "version-stack",
  "version": "2.0.0",
  "description": "File delivery server with Nginx and Node.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "migrate": "ts-node src/db/cli.ts up",
    "migrate:down": "ts-node src/db/cli.ts down",
    "migrate:status": "ts-node src/db/cli.ts status",
    "migrate:create": "ts-node src/db/cli.ts create",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "1.4.5-lts.2",
    "reflect-metadata": "^0.2.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "umzug": "^3.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
